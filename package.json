  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env.prod=true --env.analyze=true --mode production",
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js --mode production",
    "extract:messages": "npx @formatjs/cli extract 'src/**/!(*.d).{js,jsx,ts,tsx}' --ignore='**/*.d.ts' --out-file ./build/messages/messages.json",
    "translations:compile": "node utilities/mergeMessages.js",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "lint:js": "yarn eslint ./src --ext .js",
    "lint:ts": "yarn eslint ./src --ext .ts,.tsx",
    "prod": " NODE_ENV=production webpack --config config/prod.webpack.config.js --mode production",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "rm -rf ./dist && webpack --config config/dev.webpack.config.js --mode development",
    "start:standalone": "NODE_ENV=development BETA=true webpack serve --config config/standalone.dev.webpack.config.js",
    "start:beta": "NODE_ENV=development BETA=true webpack serve --config config/dev.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "build:standalone": "NODE_ENV=production webpack --config config/standalone.prod.webpack.config.js",
    "build:standalone_dev": "NODE_ENV=dev webpack --config config/standalone.dev.webpack.config.js",
    "travis:build": "NODE_ENV=production webpack --config config/prod.webpack.config.js --mode=production",
    "travis:verify": "npm-run-all travis:build lint test",
    "test": "jest --passWithNoTests --runInBand",
    "verify": "npm-run-all build lint test"
  },
  "insights": {
    "appname": "catalog"
  }
}
